package fundamentos.aulas;

import javax.swing.JOptionPane;

public class A11_StringToNumber {
	public static void main(String[] args) {
		
		String dado = JOptionPane.showInputDialog("Olá Digite um numero");
		System.out.println(dado);
		double x = Double.parseDouble(dado); // converte a saida do optionpane (que é do tipo string) em um tipo double.
		System.out.println(x/2);
		
		double y = Double.parseDouble(JOptionPane.showInputDialog("Olá digite um número: "));
	}
}
